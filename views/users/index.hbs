<body background="../images/img7.jpg" style="background-size: cover;">
<div class="container-fluid">
<h1>Users:</h1>


<p>Look Here: {{ session }}</p>

<ul>
  {{#each users}}
  	<li>
    	<h2><a href="/users/{{this.id}}/">{{this.username}}</a></h2>
    	<p>{{this.about}}</p>
    	<p>{{this.username}} has {{this.videos.length}} video(s).</p>
    	<br>
    </li>
  {{/each}}
</ul>

</div>
</body>